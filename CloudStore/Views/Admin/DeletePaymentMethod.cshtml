@model CloudStore.Models.PaymentMethod

@{
    ViewBag.Title = "Delete payment method";
}

<h1>Do you want to delete this payment method: <b>@Model.PaymentMethodName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-dark" href="@Url.Action("GetPaymentMethod", "Admin")">Back</a>
    </div>
}

