@model CloudStore.Models.PaymentMethod

@{
    ViewBag.Title = "Payment method deleting";
}

<h1>Are you sure you want to delete payment method: <b>@Model.PaymentMethodName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-outline-primary" href="@Url.Action("GetPaymentMethod", "Admin")">Back</a>
    </div>
}

