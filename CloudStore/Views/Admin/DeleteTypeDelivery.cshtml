@model CloudStore.Models.TypeDelivery

@{
    ViewBag.Title = "Delete delivery type";
}

<h1>Do you want to delete this delivery type: <b>@Model.TypeDeliveryName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-dark" href="@Url.Action("GetTypeDelivery", "Admin")">Back</a>
    </div>
}

