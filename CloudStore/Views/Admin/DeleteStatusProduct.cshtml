@model CloudStore.Models.StatusProduct

@{
    ViewBag.Title = "Status product deleting";
}

<h1>Are you sure you want to delete status product: <b>@Model.StatusProductName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-default" href="@Url.Action("GetStatusProduct", "Admin")">Back</a>
    </div>
}
