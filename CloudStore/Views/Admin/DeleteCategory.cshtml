@model CloudStore.Models.Category

@{
    ViewBag.Title = "Delete category";
}

<h1>Do you want to delete this category: <b>@Model.CategoryName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-dark" href="@Url.Action("GetCategory", "Admin")">Back</a>
    </div>
}

