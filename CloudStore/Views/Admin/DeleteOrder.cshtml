@model CloudStore.Models.Order

@{
    ViewBag.Title = "Delete order";
}

<h1>Do you want to delete this order: <b>@Model.Id</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-dark" href="@Url.Action("GetOrder", "Admin")">Back</a>
    </div>
}

